{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "manifest_version": 2,
    "name": "Youtube to mp3 || kais ",
    "version": "1.1",
    "description": "Youtube video to mp3",
    "permissions": [
        "tabs",
        "*://*.youtube.com/*",
        "http://sarojsubedi.info.np/*"
    ],
    "content_scripts": [

        {
            "matches": [
                "*://www.youtube.com/*"
            ],
            "js": ["youtube.js"],
             "css": [ "style.css" ]
        }
    ],
    "browser_action": {
       
    },
    "web_accessible_resources": [
        "youtube.css"
    ]
}